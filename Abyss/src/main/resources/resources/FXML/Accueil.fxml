<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="accueil" prefHeight="1000.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.abyss.controller.AccueilController">
   <children>
      <ImageView fx:id="logo" fitHeight="196.0" fitWidth="204.0" layoutX="877.0" layoutY="65.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/logo.png" />
         </image></ImageView>
      <VBox layoutX="875.0" layoutY="342.0">
         <children>
            <Button fx:id="jouer" mnemonicParsing="false" onAction="#commencement" prefHeight="85.0" prefWidth="200.0" style="-fx-background-color: Transparent;" text="Aventure" textAlignment="CENTER" textFill="#ffaf00">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font></Button>
            <Button fx:id="multi" mnemonicParsing="false" onAction="#multiplayer" prefHeight="85.0" prefWidth="200.0" style="-fx-background-color: Transparent;" text="Multijoueur" textAlignment="CENTER" textFill="#ffaf00">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Button>
            <Button fx:id="collection" mnemonicParsing="false" onAction="#collection" prefHeight="85.0" prefWidth="200.0" style="-fx-background-color: Transparent;" text="Collection" textAlignment="CENTER" textFill="#ffaf00">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Button>
            <Button fx:id="quiter" mnemonicParsing="false" onAction="#quitter" prefHeight="85.0" prefWidth="200.0" style="-fx-background-color: Transparent;" text="Quitter" textAlignment="CENTER" textFill="#ffaf00">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
